page {
  height: 100%;
  overflow: hidden;
  width: 100%;
}
.u-checkbox__label {
  margin-right: 0px !important;
}
.container {
  position: relative;
  height: 100%;
  width: 100%;
  overflow-y: auto;
  background: #f6f7f9;
}
.container .home_musicSrc_box {
  position: fixed;
  z-index: 999999;
  bottom: 120rpx;
}
.navTitle {
  position: absolute;
  left: 0;
  width: 100%;
  text-align: center;
  font-size: 40rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #111111;
}
.auditionListbox {
  width: 700rpx;
  background-color: #FFFFFF;
  margin-left: 25rpx;
  padding-top: 1.812rpx;
  padding-bottom: 1.812rpx;
  border-radius: 25.362rpx;
}
.auditionListbox .shareportrait {
  margin-top: 32rpx;
  display: flex;
  align-items: center;
  padding: 0px 25rpx;
  height: 36.232rpx;
  font-size: 25.362rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 36.232rpx;
}
.auditionListbox .shareportrait .audio_text {
  width: 50%;
}
.auditionListbox .shareportrait .choose_text {
  text-align: right;
  width: 50%;
}
.listcontentbox {
  margin-top: 32rpx;
