.pys_popup {
  position: fixed;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 999999;
}
.pys_popup .popup_conent {
  position: absolute;
  padding: 36.232rpx;
  top: 40%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  min-width: 500.71rpx;
  min-height: 326.087rpx;
  background: #FFFFFF;
  border-radius: 28.986rpx;
}
.pys_popup .popup_conent .closeicon {
  position: absolute;
  width: 50.725rpx;
  height: 50.725rpx;
  top: -66.232rpx;
  right: 0;
}
