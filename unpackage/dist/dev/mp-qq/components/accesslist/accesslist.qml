<view class="content"><block qq:if="{{dataList.length!==0}}"><view><block qq:for="{{dataList}}" qq:for-item="item" qq:for-index="index" qq:key="index"><view class="accesslist"><image class="accesslist_portrait" src="{{item.teacher.avatar}}" mode="scaleToFill" data-event-opts="{{[['tap',[['handleTravelTo',['$0'],[[['dataList','',index]]]]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['handleTravelTo',['$0'],[[['dataList','',index]]]]]]]}}" class="personal_Profile" bindtap="__e"><view class="name_box"><text class="nick_name u-line-1">{{item.teacher.nickname}}</text><block qq:if="{{item.teacher.sex===1}}"><image class="gender" src="/static/teacherlist/manicon.png" mode="scaleToFill"></image></block><block qq:else><block qq:if="{{item.teacher.sex===2}}"><image class="gender" src="/static/teacherlist/woman.png" mode="scaleToFill"></image></block><block qq:else><block qq:if="{{item.teacher.sex===3}}"><image class="gender" src="/static/teacherlist/qi.png" mode="scaleToFill"></image></block></block></block></view><view class="deta_text u-line-1">{{item.created_at}}</view></view><block qq:if="{{item.follow===0}}"><view data-event-opts="{{[['tap',[['handleFocus',['$0',0],[[['dataList','',index]]]]]]]}}" class="focus_btn" bindtap="__e">关注</view></block><block qq:else><view data-event-opts="{{[['tap',[['handleFocus',['$0',1],[[['dataList','',index]]]]]]]}}" class="not_concern" bindtap="__e">已关注</view></block></view></block></view></block><block qq:if="{{dataList.length===0}}"><view class="emptydata"><image class="emptydataimg" src="/static/carddetails/emptydata.png" mode="scaleToFill"></image><view class="emptydatatext">暂无数据</view></view></block></view>